@article {Wales1368,
	author = {Wales, David J. and Scheraga, Harold A.},
	title = {Global Optimization of Clusters, Crystals, and Biomolecules},
	volume = {285},
	number = {5432},
	pages = {1368--1372},
	year = {1999},
	doi = {10.1126/science.285.5432.1368},
	publisher = {American Association for the Advancement of Science},
	abstract = {Finding the optimal solution to a complex optimization problem is of great importance in many fields, ranging from protein structure prediction to the design of microprocessor circuitry. Some recent progress in finding the global minima of potential energy functions is described, focusing on applications of the simple {\textquotedblleft}basin-hopping{\textquotedblright} approach to atomic and molecular clusters and more complicated hypersurface deformation techniques for crystals and biomolecules. These methods have produced promising results and should enable larger and more complex systems to be treated in the future.},
	issn = {0036-8075},
	URL = {http://science.sciencemag.org/content/285/5432/1368},
	eprint = {http://science.sciencemag.org/content/285/5432/1368.full.pdf},
	journal = {Science}
}